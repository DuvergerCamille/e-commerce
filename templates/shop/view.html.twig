{% extends 'layout.html.twig' %}

{% block body %}
    {% for instru in listInstruments %}
    <li>{{ instru.name }} Prix: {{ instru.price }}$</li>
    {% else %}
    <li>Pas d'instrument dans cette catégorie.</li>
    {% endfor %}
    {% for sheet in listSheets %}
    <li>{{ sheet.name }} Prix: {{ sheet.price }}$</li>
    {% else %}
    <li>Pas de partition dans cette catégorie.</li>
    {% endfor %}
{% endblock %}
